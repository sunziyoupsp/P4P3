import math

def cylinder(r,h):
    volume = r * r * h * math.pi
    return round(volume,3)

r1 = float(input("Please enter the radius of a cylinder 1:"))
h1 = float(input("Please enter the height of a cylinder 1:"))

r2 = float(input("Please enter the radius of a cylinder 2:"))
h2 = float(input("Please enter the height of a cylinder 2:"))

print("The volume of the cylinder 1 is:",cylinder(r1,h1),"cubic units.")
print("The volume of the cylinder 2 is:",cylinder(r2,h2),"cubic units.")
