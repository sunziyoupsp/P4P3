import random
target = random.randint(1,100)
count = 1
while True:
    num = int(input("Enter your guess: "))
    if num < target:
        print("Your guess is too low")
    elif num > target:
        print("Your guess is too high")
    else:
        print("Correct!")
        break;
    count += 1
print("It took you",count,"guesses to get the correct number.")
